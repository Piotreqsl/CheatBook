<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AlertDialogStyle" parent="ThemeOverlay.MaterialComponents.Dialog.Alert">
        <item name="android:colorPrimary">#FFFFFF</item>
        <item name="android:colorAccent">#FFFFFF</item>
        <item name="android:textColor">#F5F4F9</item>
        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="android:background">#151528</item>
        <item name="buttonBarNegativeButtonStyle">@style/NegativeButtonStyle</item>
        <item name="buttonBarPositiveButtonStyle">@style/PositiveButtonStyle</item>
    </style>

    <style name="MyTitleTextStyle">
        <item name="android:textColor">#FFEB3B</item>
        <item name="android:fontFamily">@font/fieldbold</item>

    </style>

    <style name="NegativeButtonStyle" parent="Widget.MaterialComponents.Button.TextButton.Dialog">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:fontFamily">@font/fieldbold</item>
    </style>

    <style name="PositiveButtonStyle" parent="Widget.MaterialComponents.Button.TextButton.Dialog">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:fontFamily">@font/fieldbold</item>
    </style>

</resources>